@using Microsoft.AspNetCore.Components.Web
@namespace Runner.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@addTagHelper *, Vite.AspNetCore
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <base href="~/"/>
    <link rel="stylesheet" vite-href="~/Assets/Css/app.css" asp-append-version="true"/>
    <component type="typeof(HeadOutlet)" render-mode="Server"/>
</head>
<body class="">
<div class=""> 
    @RenderBody()
</div>
<div id="blazor-error-ui">
    <environment include="Staging,Production">
        An error has occurred. This application may no longer respond until reloaded.
    </environment>
    <environment include="Development">
        An unhandled exception has occurred. See browser dev tools for details.
    </environment>
    <a href="" class="reload">Reload</a>
    <a class="dismiss">🗙</a>
</div>
<script src="https://kit.fontawesome.com/f64cc054fb.js" crossorigin="anonymous"></script>
<script src="_framework/blazor.server.js"></script>
<script type="module" vite-src="~/Assets/Js/app.js" asp-append-version="true"></script>
<script type="module" vite-src="~/Assets/Js/Theme.js" asp-append-version="true"></script>
</body>
</html>